import React, { useState } from 'react';

const RunButton = (props) => {
    const [run, setRun] = useState(false);

    const handleClick= (e) => {
	setRun(!run);
	console.log("Set run to ", run);
    }
    
    return <button onClick={handleClick} className="RunButton">run</button>
}

export default RunButton;
